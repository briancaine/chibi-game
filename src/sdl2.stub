 (unsigned-short mod       SDL_Keysym-mod)
 (unsigned-int   unused    SDL_Keysym-unused))

(define-c-struct SDL_KeyboardEvent
 predicate:   SDL_KeyboardEvent?
 constructor: (make-SDL_KeyboardEvent type timestamp windowID state repeat)
