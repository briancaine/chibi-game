 (unsigned-int  windowID  SDL_WindowEvent-windowID)
 (unsigned-char event     SDL_WindowEvent-event)
 (int           data1     SDL_WindowEvent-data1)
 (int           data2     SDL_WindowEvent-data2))

(define-c-struct SDL_Keysym
 predicate:   SDL_Keysym?
